{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "comment-format": false,
    "ordered-imports": false,
    "no-shadowed-variable": false,
    "no-angle-bracket-type-assertion": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single"
    ],
    "no-console": [
      false,
      "log",
      "warning"
    ]
  },
  "rulesDirectory": []
}
